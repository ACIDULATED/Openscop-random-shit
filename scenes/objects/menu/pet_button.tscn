func _process(delta):
	if get_node(\"AnimationPlayer\").get_current_animation()==\"selected\":
		if get_node(\"button_face\").frame<15:
			get_node(\"button_face\").frame+=50.*delta
		else:
			get_node(\"button_face\").frame=1
	else:
		get_node(\"button_face\").frame=0
"

